{
    "text": [
        "$FILE_NAME for $PROJECT in $PATH",
        "",
        "Made by $USER_NAME",
        "Login $USER_LOGIN <$USER_EMAIL>",
        "",
        "Started on  $DATE $USER_NAME",
        "Last update $DATE $USER_NAME"
    ],
    "updateLines": [2, 5, 7],
    "datesFormat": "MM DD HH:MI:SS YYYY",
    "monthNames": ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"],
    "scopes": {
        "source.c": {
            "head": "/*",
            "body": "**",
            "tail": "*/"
        },
        "source.cpp": {
            "head": "//",
            "body": "//",
            "tail": "//"
        },
        "source.makefile": {
            "head": "##",
            "body": "##",
            "tail": "##"
        },
        "source.ocaml": {
            "head": "(*",
            "body": "**",
            "tail": "*)"
        },
        "default": {
            "head": "#",
            "body": "#",
            "tail": "#"
        }
    }
}
